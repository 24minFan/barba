{
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["add-module-exports"]
    },
    "esm": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false
          }
        ]
      ]
    },
    "umd": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": "commonjs",
            "targets": {
              "browsers": ["last 2 versions", "ie >= 11"]
            }
          }
        ]
      ]
    },
    "test": {
      "presets": ["@babel/env"]
    }
  }
}
