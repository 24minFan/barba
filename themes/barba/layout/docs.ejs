<% const docs = [
  {
    name: 'Get Started',
    url: '/docs/getstarted',
  },
  {
    name: 'User Guide (@core)',
    url: '/docs/userguide/markup',
    subdocs: [
      {
        name: 'Markup',
        url: '/docs/userguide/markup',
      },
      {
        name: 'Syntax',
        url: '/docs/userguide/syntax',
      },

    ],
  },
  {
    name: 'Plugins',
    url: '/docs/plugins/css',
    subdocs: [
      {
        name: 'CSS',
        url: '/docs/plugins/css',
      },
      {
        name: 'Router',
        url: '/docs/plugins/router',
      },
      {
        name: 'Prefetch',
        url: '/docs/plugins/prefetch',
      },
      {
        name: 'Transitions',
        url: '/docs/plugins/transitions',
      },
      {
        name: 'Head',
        url: '/docs/plugins/head',
      },
    ],
  },
] %>

<section class="docs">
  <div class="docs__side-nav">
    <ul class="docs__side-nav__list">
      <% docs.forEach(function(doc) { %>
      <li>
        <a href="<%- doc.url %>">
          <h1 class="docs__side-nav__title"><%- doc.name %></h1>
        </a>
        <ul class="docs__side-nav__sublist">
          <% if (doc.subdocs) { %>
          <% doc.subdocs.forEach(function(subdoc) { %>
          <li>
            <a href="<%- subdoc.url %>">
              <h2 class="docs__side-nav__subtitle"><%- subdoc.name %></h2>
          </li>
          <% }); %>
          <% }; %>
        </ul>
      </li>
      <% }); %>
    </ul>
  </div>

  <div class="docs__markdown">
    <%- page.content %>
  </div>
  </div>
</section>
